  </head>
  <body>
    <div class="title">
      <h1>-</h1>
    </div>
    <div class="body">
      <% articles.forEach(function(title) { %>
      <div class="article-listing">
        <a class="link" href="articles/<%= title._id %>"><%= title.title %></a>
